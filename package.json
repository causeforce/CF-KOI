{
  "name": "cfkoi-web-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "del-cli public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/js/app/**/*.js' 'public/js/app/**/*.js.map' '!public/js/app/bundle.js' '!public/js/app/*.chunk.js' 'assets/app/**/*.ngfactory.ts' 'assets/app/**/*.shim.ts' 'assets/app/**/*.ngsummary.json' 'assets/app/**/*.ngstyle.ts'"
  },
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/platform-server": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@angular/upgrade": "^4.4.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.15.2",
    "bootstrap": "^4.0.0-beta.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.0.11",
    "hbs": "~3.1.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^5.7.0",
    "moment": "^2.19.2",
    "mongoose": "^4.13.4",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "~1.6.1",
    "reflect-metadata": "^0.1.3",
    "request": "^2.83.0",
    "rxjs": "^5.5.2",
    "serve-favicon": "~2.3.0",
    "uglify-js": "^3.3.11",
    "zone.js": "^0.8.5"
  },
